#include <stdio.h>
#include <iostream>
 
using namespace std;
int main()
{
	int a=0;
	cin >> a;
	for(;a>0;a--){
		string x;
		cin >> x;
		int t[256];
		for(int i=0;i<256;i++){		//zerowanie tablicy t
			t[i]=0;
		}
		for(int i=0;x.length()>i;i++){		//wpisywanie ilo≈õci liter do t
			t[(int)x[i]]++;
		}
		string y;
		for(int i=0;i<256;i++){
			for(int n=0;t[i]>n;n++){
				y+=(char)i;
			}
		}
		cout << y << endl;
	}
}
