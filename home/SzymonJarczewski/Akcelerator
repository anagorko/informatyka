#include <iostream>
using namespace std;

int main(){
	int n;		//liczba cząsteczek
	cin >> n;
	int t[n-1];		//kolejne prędkości cząstek
	for(int i=0; i<n; i++){		//wpisywanie do tablicy
		int j;		//liczba, która zaraz będzie wpisana
		cin >> j;
		t[i]=j;
	}
	int q;		//liczba zapytań
	cin >> q;
	for(; q>0; q--){
		int p;		//zapytanie
		cin >> p;
		int s=0;		//początek
		int e=n-1;		//koniec
		int x=(e+s)/2;		//sprawdzana wartość
		for(;;){
			if(t[x]>p){		//jeśli x jest za duży
				e=x-1;
				x=(e+s)/2;
			}
			if(t[x]<p){		//jeśli x jest za mały
				s=x+1;
				x=(e+s)/2;
			}
			if(t[x]==p){		//jeśli x trafi
				for(int i=x; ;){		//sprawdzamy górny przedział
					}
				for(int i=x; ;){		//sprawdzamy dolny przedział
					}
			}
		}
	}
}
